def gcd(a, b):
    while b != 0:
        rem = a % b
        a = b
        b = rem
    return (a==1)
a = int(input("Enter the num: "))
for i in range(5, a):
    for j in range(4, i):
        for k in range(3, j):
            if (j*j + k*k == i*i and gcd(i,j) and gcd(j,k)  and gcd(i,k)):
                print(i, j, k)
